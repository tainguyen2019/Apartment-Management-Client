{
  "plugins": ["prettier","jest"],
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "react/jsx-filename-extension": ["warn", { "extensions": [".tsx"] }],
    "jest/no-focused-tests": "error",
    "jest/valid-title": "off"
  }
}
